#!/usr/bin/env bash

versions_list=$(
  curl https://sourceforge.net/projects/sbcl/files/sbcl/ |
  grep -Eoh '/[0-9]+\.[0-9]+(\.[0-9]+)/download'         | # Find version numbers
  sed -e 's/\///g'                                       | # Remove the slashes
  sed -e 's/download//g'                                 | # Remove "download"
  sort -t '.' -k 1,1n -k 2,2n -k 3,3n )                    # Sort by version number

versions=""

for version in ${versions_list}
do
  versions="${versions} ${version}"
done

echo "$versions"
